module rand(
    input clk,
	 input en,
	 input rst,
	 input [4:0]dodge,
	 output [4:0]num);
	 
	 reg [4:0] place;
	 
	 always @(posedge clk or negedge rst) begin
	     if (rst == 1'b0) begin
		      num <= dodge;
			end else begin
			   place = {dodge[2] ~^ dodge[3], !dodge[4], dodge[1:0], dodge[4] ^ dodge[2]};
				num <= place % dodge;
			end
	 end 
	  
	 